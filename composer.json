{
  "name": "dmt-software/import-reader",
  "description": "Read parts of a file/stream into a series of objects or arrays",
  "keywords": ["import", "file", "stream", "reader", "iterator", "csv", "json", "xml"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bas de Mes",
      "email": "bas@dmt-software.nl"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "pcrov/jsonreader": "^1.0",
    "dmt-software/xml-parser": ">=0.4"
  },
  "autoload": {
    "psr-4": {
      "DMT\\Import\\Reader\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "psr/log": "^1.1",
    "jms/serializer": "^3.18"
  },
  "autoload-dev": {
    "psr-4": {
      "DMT\\Test\\Import\\Reader\\": "tests/"
    }
  },
  "suggest": {
    "jms/serializer": "to go through a collection of deserialized objects from an import file."
  }
}
